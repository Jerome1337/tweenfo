{
  "name": "tweenfo",
  "version": "0.1.0",
  "author": {
    "name": "Jérôme Pogeant",
    "url": "https://jeromepogeant.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jerome1337/tweenfo.git"
  },
  "license": "MIT",
  "description": "Tweenfo is a Twitter complementary application to find statistic about a Twitter account.",
  "keywords": [
    "svelte",
    "Twitter",
    "stats",
    "statistics",
    "API",
    "Tweets"
  ],
  "main": "src/main.js",
  "scripts": {
    "dev": "run-p start:dev watch:css",
    "build": "npm run build:tailwindcss && sapper build",
    "export": "npm run build:tailwindcss && sapper export",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "watch:css": "postcss static/tailwind.pcss -o static/build.css -w",
    "build:css": "NODE_ENV=production postcss static/tailwind.pcss -o static/build.css",
    "lint": "eslint --cache src/**/*.{js,svelte}",
    "lint:fix": "eslint --cache --fix src/**/*.{js,svelte}",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/runtime": "^7.1.5",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "chokidar": "^3.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.2",
    "postcss": "^7.0.32",
    "postcss-cli": "^8.0.0",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^2.26.11",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.6",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.2.1",
    "tailwindcss": "^1.8.5"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "express-session": "^1.16.2",
    "marked": "^1.0.0",
    "node-fetch": "^2.1.2",
    "polka": "^0.5.2",
    "session-file-store": "^1.2.0",
    "svelte-i18n": "^3.0.4",
    "sirv": "^1.0.0"
  }
}
